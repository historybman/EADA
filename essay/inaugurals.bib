@phdthesis{friedman_herlihy_tattooed_2012,
	address = {United States -- Illinois},
	type = {Ph.{D}.},
	title = {Tattooed transculturites: {Western} expatriates among {Amerindian} and {Pacific} {Islander} societies, 1500–1900},
	copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
	shorttitle = {Tattooed transculturites},
	url = {http://search.proquest.com/docview/1030969688/abstract/93B79DC03D874C1FPQ/1},
	abstract = {Through the biographies of eighty-five travellers, this dissertation traces the history of the acquisition of tattoos as markers of cultural hybridity crafted through processes of initiation and transculturalization. A subset of "Western" travelers (mainly English, French, American, and Spanish—although several other nationalities are present here as well) became immersed in Amerindian and Pacific Islander societies between 1500 and 1900. When certain conditions were met—the presence of frontier space, codified cultural traditions of adoption, tattoos that could be given to those of non-natal heritage, and personalities open to identity change—these travelers had their bodies indelibly inscribed with non-Western motifs that represented, or even enabled, their new identities.
These transculturites are contrasted with travelers who acquired souvenir tattoos as exotic tokens or memorial reminiscences. Trends in the practice are traced as transculturalization and its concomitant tattooing began to wane in North America with a loss of frontier spaces toward the end of the eighteenth century and disappeared completely by the end of the nineteenth due to religious and racist objections. Reasons for a shift in the nationality of those who transculturalized also emerges from the evidence—from Spanish to French to English (and to a certain extent American).
Beyond the specific history of tattooing, this dissertation offers a nuanced account of cross-cultural encounters that shows that such interactions were often two-way and that those of European heritage did not always or necessarily assume the dominant position in a hierarchical relationship. It also, importantly, debunks the "Cook myth"—the notion that modern Western tattooing originated from contact with Pacific Islanders starting in the late eighteenth century—as well as the notion that tattoos on Westerners were usually seen as somehow stigmatic or otherwise negative during the period 1500-1900.},
	language = {English},
	urldate = {2016-08-02},
	school = {The University of Chicago},
	author = {Friedman Herlihy, Anna Felicity},
	year = {2012},
	keywords = {Body art, Communication and the arts, Cultural hybridity, Native Americans, Pacific Islands, Social sciences, Tattoos},
	file = {Friedman Herlihy_2012_Tattooed transculturites.pdf:/Users/rmorriss/Box Sync/Work/Research/Zotero/Download_articles_books/Friedman Herlihy_2012_Tattooed transculturites.pdf:application/pdf}
}

