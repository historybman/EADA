@article{breen_elks_2013,
	title = {“{The} {Elks} {Are} {Our} {Horses}”: {Animals} and {Domestication} in the {New} {France} {Borderlands}*},
	volume = {3},
	issn = {1877-0703},
	shorttitle = {“{The} {Elks} {Are} {Our} {Horses}”},
	url = {http://booksandjournals.brillonline.com/content/journals/10.1163/18770703-00303002},
	doi = {10.1163/18770703-00303002},
	abstract = {Historians have long overlooked the role played by animals in cross-cultural interactions in the American borderlands. Yet domesticated animals - and the social practices that accompanied them - were central both to the ‘civilizing mission’ of colonizers and to indigenous American resistance. This paper examines these themes within the context of the Great Lakes and Upper Mississippi region between 1670 and 1730. Drawing evidence from Algonquian and Iroquoian languages and cultural practices as well as from the accounts of French missionaries and voyageurs, I show that the indigenous peoples of the Pays d’en Haut rejected the positive connotations that domestication held for Europeans, and instead equated domestication with enslavement. The resulting conflicts between conceptions of nature, ownership and tameness had an enduring influence on European-Indian relations. Although this study examines specific patterns of interaction on the New French frontier, it also raises broad questions relating to environmental history and European-indigenous interactions throughout the New World.},
	number = {2-3},
	urldate = {2015-04-03},
	journal = {Journal of Early American History},
	author = {Breen, Benjamin},
	month = jan,
	year = {2013},
	pages = {181--206},
	file = {1459489.pdf:/Users/rmorriss/Library/Application Support/Firefox/Profiles/qj3yma29.default/zotero/storage/KS9K5BA5/1459489.pdf:application/pdf;Snapshot:/Users/rmorriss/Library/Application Support/Firefox/Profiles/qj3yma29.default/zotero/storage/DM4FJCGM/18770703-00303002.html:text/html}
}

@article{ekberg_1767_2012,
	title = {The 1767 {Dufossat} {Maps} of {St}. {Louis}},
	issn = {01989375},
	abstract = {The article discusses the early history of St. Louis, Missouri, commenting on the appearance of the name "La Joie" in 1767 maps by Guy Dufossat, a French military engineer. It examines the life of Jean Salé, called La Joie, or "the joyful one," a French-born trader, noting his marriage to María Rosa Villalpando, a woman of Spanish-Indian descent, and his return to France in 1792. The author also addresses the exploration of the area and the founding of St. Louis in 1763 by French fur trader Pierre Laclède Liguest and his assistant Auguste Chouteau.},
