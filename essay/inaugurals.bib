@article{anderson_animals_2002,
	title = {Animals into the {Wilderness}: {The} {Development} of {Livestock} {Husbandry} in the {Seventeenth}-{Century} {Chesapeake}},
	volume = {59},
	issn = {00435597},
	shorttitle = {Animals into the {Wilderness}},
	abstract = {Examines animal husbandry practices in 17th-century colonial Virginia and Maryland, explaining the reasons for the Chesapeake-area farmers' divergence from the traditional English mode of cattle control and raising. The availability of grazing land and the value and ease of raising crops such as tobacco facilitated the development of less labor-intensive free-range husbandry. Royal governors, proprietors of large land grants, small farmers, and decisionmakers in London had little legal precedent regarding disposition of livestock that survived in "the wilde" and later became the subjects of conflicting claims. Such issues diminished considerably by the 18th century as the international market for grain and the advent of cheap labor in the form of slaves made wheat, maize, and other grain crops more profitable. These factors and reduced amounts of open grazing land forced a readaptation to traditional animal husbandry practices in the Chesapeake, as free-range husbandry found itself displaced to large regions of meadowlands in western portions of the colonies.},
	number = {2},
	journal = {William \& Mary Quarterly},
	author = {Anderson, Virginia DeJohn},
	month = apr,
	year = {2002},
	keywords = {agriculture, ANIMAL culture, CHESAPEAKE Bay Region (Md. \& Va.), CHESAPEAKE Bay Region (Md. \& Va.) -- History, LIVESTOCK -- Breeding, MARYLAND, UNITED States -- History -- Colonial period, ca. 1600-1775, VIRGINIA},
	pages = {377}
