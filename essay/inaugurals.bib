@article{soudiere_devenir_2009,
	series = {nouvelle serie},
	title = {Devenir historien du climat ? {Entretien} avec {Emmanuel} {Le} {Roy} {Ladurie}},
	volume = {39},
	copyright = {Copyright © 2009 Presses Universitaires de France},
	issn = {0046-2616},
	shorttitle = {Devenir historien du climat ?},
	url = {http://www.jstor.org/stable/40991259},
	abstract = {Qu'est-ce qui peut bien pousser, dans les années 1950, un historien rural à s'intéresser au climat ? Comment situer cette posture dans la mouvance marxiste de l'époque ? Le principal souci d'Emmanuel Le Roy Ladurie a été de croiser l'étude de la production agricole avec les fluctuations de la population. Pionnier français de l'histoire du climat, l'historien s'en explique au cours de deux rencontres qu'il a eues avec Martin de la Soudière, Martine Tabeaud et Anouchka Vasak en 2008. Il en ressort en particulier l'apport, pour lui décisif, de la fréquentation d'autres disciplines comme la glaciologie, la météorologie ainsi que de la littérature. What are the mean reasons to study climate for a rural history researcher during the sixteen's ? What slot does this attitude fill in the Marxist way of thinking history at that time ? His most important purpose was to compare agricultural productions and population fluctuations. Emmanuel Le Roy Ladurie, who is the French pioneer of climate history, met Martin de la Soudière, Martine Tabeaud and Anouchka Vasak in 2008. He answered a lot of questions and was insistent about practicing interdisciplinarity, for example with glaciology, meteorology and literature. Was hat wohl einen Historiker in den 1950er Jahren dazu gebracht, sich speziell für das Klima zu interessieren ? Und in welchem Zusammenhang stand sein Interesse zur damaligen marxistischen Bewegung ? „ Zunächst ging mir darum, landwirtschaftliche Produktion und Bevölkerungsfluktuation zueinander in Bezug zu setzen", erklärt Emmanuel Le Roy Ladurie (französischer Pionier der Klimageschichte) seinen Werdegang in den beiden Interviews, die Martin de la Soudière, Martine Tabeaud und Anouchka Vasak 2008 mit ihm gefuhrt haben. Besonders wichtig ist für ihn bis heute die Interdisziplinarität seiner Arbeit, die zum Beispiel zu einer Zusammenarbeit mit der Gletscherkunde, der Meteorologie oder der Literaturwissenschaft geführt hat.},
	number = {4},
	urldate = {2015-09-09},
	journal = {Ethnologie française},
	author = {Soudière, Martin de la and Tabeaud, Martine and Vasak, Anouchka and Ladurie, Emmanuel Le Roy},
	month = oct,
	year = {2009},
	pages = {639--644}
}

