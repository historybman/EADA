@book{din_spaniards_1999,
	title = {Spaniards, {Planters}, and {Slaves}: {The} {Spanish} {Regulation} of {Slavery} in {Louisiana}, 1763-1803},
	isbn = {978-0-89096-904-5},
	shorttitle = {Spaniards, {Planters}, and {Slaves}},
	abstract = {Spaniards, Planters, and Slaves is a provocative look at the institution of slavery and its function as a part of Louisiana's culture during the years of Spanish rule. Gilbert C. Din challenges the idea that conditions under the Spaniards differed little from the years of French rule and examines how local culture merged with colonial government and residual laws to create a slave system unlike any other in the Deep South.Din presents many aspects of the slavery issue, including a look at the French system, conflicts between planters who favored the established system and governors who promoted the less stringent Spanish laws, and the political favoritism that sought to benefit the wealthy New Orleans district. Din also discusses the role of the Catholic Church.Using government documents from archives in Spain and Louisiana, Din paints a historically accurate portrait of a time when the blended culture of the eighteenth-century colony resulted in conflict and turmoil. Most important are the Papeles Procedentes de la Isla de Cuba, a collection of colonial documents that illustrate not only the actions but also the personalities of the governors and how they implemented changes and handled problems within the slave system.Spaniards, Planters, and Slaves is the first in its field to capture the years of Spanish rule as a specific and unique point in Louisiana's history of slavery. Din's research uncovers both the complexities of the slavery issue and the Spanish heritage that ultimately helped to shape the slave system of the future state. It is an ideal study for anyone interested in the history of both colonial Louisiana and slavery itself.},
	language = {en},
	publisher = {Texas A\&M University Press},
	author = {Din, Gilbert C.},
	year = {1999}
}

@phdthesis{muller_empire_2010,
	address = {United States -- New Jersey},
	type = {Ph.{D}.},
	title = {An empire of subjects: {Unities} and disunities in the {British} {Empire}, 1760--1790},
	copyright = {Copyright ProQuest, UMI Dissertations Publishing 2010},
	shorttitle = {An empire of subjects},
	url = {http://search.proquest.com.proxy2.library.illinois.edu/docview/821789856/13B858F10A36A9810C8/1?accountid=14553},
	abstract = {Often hailed as an age of revolution, even of global crisis, the late eighteenth century was also an era when royal authority was acknowledged and subjecthood was affirmed. Subjects, as well as citizens, insisted on political, economic, and legal concessions and inserted their distinct voices into the annals of history through languages that expressed both deference and demands. Challenging scholarship that views subjects as inferior precursors to citizens, this dissertation argues that subjecthood served as an organizing and contested principle of the eighteenth century.
Focusing on the aftermath of the Seven Years' War (1756-1763), this project uncovers some of the rights that were eventually, though unevenly, associated with British subjecthood. During this period, questions about the status, duties, and rights of their multi-religious, multi-national, and multi-racial "new subjects" became increasingly pressing as British colonial officials sought to consolidate control over numerous conquered and ceded territories. The juxtaposition of case studies as disparate as Grenada, Quebec, Minorca, Gibraltar, and Calcutta highlights the importance of the language of subjecthood at the highest levels of imperial governance and in the daily lives of colonial subjects. Examining struggles over the meanings of subject status in each of these territories demonstrates how inhabitants and administrators wielded this language to secure political, economic, and legal privileges.
Ultimately, subjecthood both unified and divided a transformed British Empire. Individuals living across a variety of territories and under distinctive legal systems could lay claim to its protections. At the same time, since the rights of subjects remained relatively undefined by law, the privileges due subjects were variously interpreted and granted to greater or lesser extents across the colonies. The language of subjecthood was never merely formulaic, but rather, inhabitants actively and provocatively used it to make demands in response to local exigencies and opportunities. The story of subjecthood reveals an empire in which individuals constantly challenged the boundaries of belonging, inhabitants put forth claims that were different from those made within the realm, and authority was often negotiated more than it was imposed.},
	language = {English},
	urldate = {2013-01-09},
	school = {Princeton University},
	author = {Muller, Hannah Weiss},
	year = {2010},
	file = {Snapshot:/Users/rmorriss/Library/Application Support/Firefox/Profiles/qj3yma29.default/zotero/storage/J6EGAM2A/1.html:text/html}
}

