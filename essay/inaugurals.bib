@article{mt.pleasant_food_2016,
	title = {Food {Yields} and {Nutrient} {Analyses} of the {Three} {Sisters}: {A} {Haudenosaunee} {Cropping} {System}},
	volume = {7},
	copyright = {Copyright (c) 2016 Jane Mt.Pleasant},
	issn = {2159-8126},
	shorttitle = {Food {Yields} and {Nutrient} {Analyses} of the {Three} {Sisters}},
	url = {http://ojs.ethnobiology.org/index.php/ebl/article/view/721},
	doi = {10.14237/ebl.7.1.2016.721},
	abstract = {Scholars have studied The Three Sisters, a traditional cropping system of the Haudenosaunee (Iroquois), from multiple perspectives. However, there is no research examining food yields, defined as the quantities of energy and protein produced per unit land area, from the cropping system within Iroquoia. This article compares food yields and other nutrient contributions from the Three Sisters, comprised of interplanted maize, bean and pumpkin, with monocultures of these same crops. The Three Sisters yields more energy (12.25 x 106 kcal/ha) and more protein (349 kg/ha) than any of the crop monocultures or mixtures of monocultures planted to the same area. The Three Sisters supplies 13.42 people/ha/yr. with energy and 15.86 people/ha/yr. with protein. Nutrient contents of the crops are further enhanced by nixtamalization, a traditional processing technique where maize is cooked in a high alkaline solution. This process increases calcium, protein quality, and niacin in maize.},
	language = {en},
	number = {1},
	urldate = {2017-02-22},
	journal = {Ethnobiology Letters},
	author = {Mt.Pleasant, Jane},
	month = nov,
	year = {2016},
	keywords = {Haudenosaunee, Iroquois, Nixtamalization, Nutrition, Three Sisters},
	pages = {87--98},
	file = {Mt.Pleasant_2016_Food Yields and Nutrient Analyses of the Three Sisters.pdf:/Users/rmorriss/Box Sync/Work/Research/Zotero/Download_articles_books/Mt.Pleasant_2016_Food Yields and Nutrient Analyses of the Three Sisters.pdf:application/pdf;Snapshot:/Users/rmorriss/Library/Application Support/Firefox/Profiles/qj3yma29.default/zotero/storage/FIA6RSS3/721.html:text/html}
