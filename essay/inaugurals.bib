	abstract = {Recent criticisms of the use of historically and ethnographically recorded conflicts as models for warfare in prehistoric times force archaeologists to reexamine assumptions about the frequency, severity, and effects of intergroup fighting. In eastern North America, skeletons of victims and palisaded settlements—the only information consistently available on intergroup hostilities—indicate that the prevalence of conflicts varied greatly over time and space. Occasionally the attacks, typically ambushes of small numbers of people, cumulatively resulted in numerous casualties. Variation in palisade strength is consistent with the organizational structure and warrior mobilization potential of late prehistoric societies in different parts of the Eastern Woodlands.},
	number = {2},
	urldate = {2016-09-20},
	journal = {Journal of Archaeological Research},
	author = {Milner, George R.},
	month = jun,
	year = {1999},
	keywords = {ARCHAEOLOGY, eastern North America, MILITARY art \& science, North America, palisades, PREHISTORIC antiquities, PREHISTORIC warfare, prehistory, trauma, warfare},
	pages = {105--151},
	file = {Milner_1999_Warfare in Prehistoric and Early Historic Eastern North America.pdf:/Users/rmorriss/Box Sync/Work/Research/Zotero/Download_articles_books/Milner_1999_Warfare in Prehistoric and Early Historic Eastern North America.pdf:application/pdf}
}

@article{anderson_examining_1991,
	title = {{EXAMINING} {PREHISTORIC} {SETTLEMENT} {DISTRIBUTION} {IN} {EASTERN} {NORTH} {AMERICA}},
	volume = {19},
	issn = {0360-1021},
	url = {http://www.jstor.org/stable/40914327},
	abstract = {Two methods of reconstructing prehistoric settlement distributions in eastern North America are illustrated. The distribution of Paleoindian projectile points from across the region is examined, employing primary artifactual data at a countylevel scale. Paleoindian points occur unevenly across the landscape, with pronounced concentrations in some areas, notably along the Mississippi, Tennessee, Cumberland, and Ohio Rivers, and along portions of the Atlantic and Gulf seaboards. The occurrence of these artifacts prompts a reconsideration of traditional models of Paleoindian settlement, the appearance of regional traditions, and the emergence of what is known as the Archaic stage or adaptation. Phase distribution maps encompassing the Eastern Woodlands were also prepared for the late prehistoric period, at A.D. 900-1100, 1250-1300, 1450, and 1540. Phase boundaries in this case were intuitively delimited, based on information provided by ca. 50 archeologists from across the region. The distribution of these phases over the landscape, and particularly their appearance and disappearance, provide clues of value to understanding the territorial behavior, political stability, and longevity of complex late prehistoric societies. The two projects highlight the role and importance of cooperative regional analyses. The distributional information, furthermore, offers as much insight about the current state of archeological knowledge as it does about where prehistoric peoples were actually living on the landscape.},
	urldate = {2016-09-20},
	journal = {Archaeology of Eastern North America},
	author = {Anderson, David G.},
	year = {1991},
	pages = {1--22},
	file = {JSTOR Full Text PDF:/Users/rmorriss/Library/Application Support/Firefox/Profiles/qj3yma29.default/zotero/storage/PQ6DDH68/Anderson - 1991 - EXAMINING PREHISTORIC SETTLEMENT DISTRIBUTION IN E.pdf:application/pdf}
}

