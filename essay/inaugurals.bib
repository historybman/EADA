@article{esarey_bold_1998,
	title = {{THE} {BOLD} {COUNSELOR} {PHASE} {OF} {THE} {CENTRAL} {ILLINOIS} {RIVER} {VALLEY}: {ONEOTA}'{S} {MIDDLE} {MISSISSIPPIAN} {MARGIN}},
