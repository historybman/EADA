}

@article{gibbon_cultural_1972,
	title = {Cultural {Dynamics} and the {Development} of the {Oneota} {Life}-{Way} in {Wisconsin}},
	volume = {37},
	copyright = {Copyright © 1972 Society for American Archaeology},
	issn = {0002-7316},
	url = {http://www.jstor.org/stable/278204},
	doi = {10.2307/278204},
	abstract = {An evolutionary and ecological approach is used in an attempt to define the causes and motivations involved in the process of Oneota development in Wisconsin. A primary thesis of this paper is that the Oneota life-way emerged from a Woodland base modified in the direction of the Mississippian pattern. The rapid appearance of a qualitatively distinctive pattern of socio-political organization, the multi-band or "tribal," is thought to be responsible in part for the difficulty in tracing this development. It is suggested that this level of integration became firmly established as the dominant socio-political structure throughout the northeastern United States by A.D. 1100-1200. Early Oneota settlements (A.D. 900-1300) are then contrasted to late Oneota settlements (A.D. 1300-1600) in Wisconsin and a difference in cultural ecological adaptation is suggested. Examples of organizational change in other cultures are briefly illustrated to suggest that the pattern of development may be at least in part recurrent.},
	number = {2},
	urldate = {2013-03-20},
	journal = {American Antiquity},
	author = {Gibbon, Guy E.},
	month = apr,
	year = {1972},
	note = {ArticleType: research-article / Full publication date: Apr., 1972 / Copyright © 1972 Society for American Archaeology},
	pages = {166--185},
