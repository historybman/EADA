@article{epp_way_1988,
	title = {{WAY} {OF} {THE} {MIGRANT} {HERDS}: {DUAL} {DISPERSION} {STRATEGY} {AMONG} {BISON}},
	volume = {33},
	copyright = {Copyright © 1988 Maney Publishing},
	issn = {0032-0447},
	shorttitle = {{WAY} {OF} {THE} {MIGRANT} {HERDS}},
	url = {http://www.jstor.org.proxy2.library.illinois.edu/stable/25668771},
	abstract = {Prehistoric human seasonal movements on the northern Great Plains frequently are explained in relation to postulated bison movement patterns. Two contradictory hypotheses regarding seasonality of bison movements have emerged in the archaeological literature, with consequent implications for explanations of human demographic relationships. One postulate is that bison herds migrated regularly between woodland and grassland environments with the seasons. The opposing postulate is that bison did not migrate, engaging in no more than erratic seasonal movements. Based on recorded historical observations and recent scientific data, these interpretations have had a powerful influence on explanations of prehistoric human movement and settlement patterns, at times leading to active debate. Recent ecological evidence indicates that a common foraging strategy among bovines employs a dual dispersion practice — some populations migrate to grasslands seasonally while others remain sedentary in wooded, ecologically varied locations. In other words, migratory and non-migratory behavior occur at the same time among herds which share the wooded ecosystems. Historical and ecological data suggest that the bison of the northern Great Plains employed such dual dispersion behavior, utilizing ecological niche opportunities. A human demographic implication of this ecological observation favors a model of human bands often seeking partially wooded, ecologically varied locations for habitation, venturing into open grasslands seasonally when the migrating herds were there. These wooded habitats also were those preferred by the more predictable, less migratory bison herds. These habitats also supplied other important resources such as wood, vegetable foods, deer and smaller game.},
	number = {121},
	urldate = {2014-06-23},
	journal = {Plains Anthropologist},
	author = {Epp, Henry T.},
	month = aug,
	year = {1988},
	pages = {309--320},
	file = {JSTOR Full Text PDF:/Users/rmorriss/Library/Application Support/Firefox/Profiles/qj3yma29.default/zotero/storage/9QIDJ4QR/Epp - 1988 - WAY OF THE MIGRANT HERDS DUAL DISPERSION STRATEGY.pdf:application/pdf}
}

