@article{din_immigration_1969,
	title = {{THE} {IMMIGRATION} {POLICY} {OF} {GOVERNOR} {ESTEBAN} {MIRO} {IN} {SPANISH} {LOUISIANA}},
	volume = {73},
	issn = {0038478X},
	abstract = {After the 1760's Spain's policy of immigration into Louisiana increasingly reflected the conviction that the area, if well populated with loyal and Catholic settlers, would serve as an ideal and necessary buffer province. The importance of Esteban Miró (Governor of Louisiana, 1782-92) in the formulation of policy has been grossly overlooked. Traces the development of Miró's plan to encourage the retention of Anglo-Americans in the area and to entice disaffected Protestant frontier Americans into the fold. His design revolved around several specific points: Spain could not financially shoulder the burden of transporting loyal, Catholic immigrants from Europe; Protestant Americans were near at hand, and with the institution of parishes and priests to administer to and educate them, they would soon become loyal subjects; the colonization effort must not fall into the hands of land-hungry entrepreneurs who would promise their clients self-government and freedom of religion, thereby creating inside the province a threat already present outside of it. Indeed, it was against this threat that the idea of a buffer province had originally been conceived. Intrigues of the power-seeking Frenchman Pierre Wouves d'Argès, of the American adventurer and western separatist James K. Wilkinson, and of the entrepreneur Colonel George Morgan could not deflect Miró from his plan. Although his proposals were often adopted and transmitted to the crown by others, the inspiration was Miró's. The Governor retired in 1792 and his policy gave way under the pressures of Spain's other commitments, as well as the resulting upheavals of the French Revolution.},
	number = {2},
	journal = {Southwestern Historical Quarterly},
	author = {Din, Gilbert C.},
	month = oct,
	year = {1969},
	pages = {155--175},
	file = {EBSCO Full Text:/Users/rmorriss/Library/Application Support/Firefox/Profiles/qj3yma29.default/zotero/storage/TQSQTQE4/ContentServer.html:text/html}
}

