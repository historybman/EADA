@article{aubert_blood_2004,
	title = {"{The} {Blood} of {France}": {Race} and {Purity} of {Blood} in the {French} {Atlantic} {World}},
	volume = {61},
	issn = {00435597},
	shorttitle = {"{The} {Blood} of {France}"},
	abstract = {Reexamines policies and practices in France's Atlantic colonies of the 17th and 18th centuries, revealing official discourse and decisionmaking grounded in a concern to preserve the racial "purity" of the French colonists. The author contradicts accepted historiography that found race-based French policies regarding intermarriage and sexual intercourse with blacks not emerging until the late 18th century, and then only in metropolitan France and the Caribbean. The article traces the much earlier development of official circumscription of interracial sexual union between French men and African-origin and American Indian women in Canada and in Louisiana. The 1724 Code Noir in Louisiana prohibited marriage or cohabitation with black women and was more restrictive than the Caribbean-area 1685 Code Noir. Colonial and metropolitan officials became increasingly less tolerant of interracial sex as early as the late 17th century, when the push for orderly plantation colonies became a priority with Paris. This reflected fear that "corrupted" French blood, in the form of mixed-race progeny, would jeopardize order through increased crime and even revolt, while threatening the racial integrity of the French colonial population.},
	number = {3},
	journal = {William \& Mary Quarterly},
	author = {Aubert, Guillaume},
	month = jul,
	year = {2004},
	pages = {439--478},
	file = {JSTOR Full Text PDF:/Users/rmorriss/Library/Application Support/Firefox/Profiles/qj3yma29.default/zotero/storage/GSN6C6GF/Aubert - 2004 - The Blood of France Race and Purity of Blood in.pdf:application/pdf}
