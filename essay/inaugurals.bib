	pages = {626--647},
	file = {Full Text PDF:/Users/rmorriss/Library/Application Support/Firefox/Profiles/qj3yma29.default/zotero/storage/KI9SSWRP/Anderson - 2006 - Evolution and origin of the Central Grassland of N.pdf:application/pdf;Full Text PDF:/Users/rmorriss/Library/Application Support/Firefox/Profiles/qj3yma29.default/zotero/storage/HW3A85Q5/Anderson - 2006 - Evolution and origin of the Central Grassland of N.pdf:application/pdf;Full Text PDF:/Users/rmorriss/Library/Application Support/Firefox/Profiles/qj3yma29.default/zotero/storage/D68NK74P/Anderson - 2006 - Evolution and origin of the Central Grassland of N.pdf:application/pdf;Snapshot:/Users/rmorriss/Library/Application Support/Firefox/Profiles/qj3yma29.default/zotero/storage/CZC7VDN6/1095-5674(2006)133[626EAOOTC]2.0.html:text/html;Snapshot:/Users/rmorriss/Library/Application Support/Firefox/Profiles/qj3yma29.default/zotero/storage/JD67HDIM/1095-5674(2006)133[626EAOOTC]2.0.html:text/html;Snapshot:/Users/rmorriss/Library/Application Support/Firefox/Profiles/qj3yma29.default/zotero/storage/DGWHBQFG/1095-5674(2006)133[626EAOOTC]2.0.html:text/html}
