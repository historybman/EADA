	year = {2012},
	keywords = {AMERICAN bison, climate\_history\_overview, CLIMATIC changes -- History, FAMINES -- History, Great Plains, GREAT Plains -- Environmental conditions, PLAINS peoples (North American peoples), SMALLPOX -- History},
	pages = {365--403}
}

