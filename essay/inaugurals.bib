@article{guyette_fire_2003,
	title = {Fire and {Human} {History} of a {Barren}-{Forest} {Mosaic} in {Southern} {Indiana}},
	volume = {149},
	issn = {0003-0031},
	url = {http://www.bioone.org/doi/abs/10.1674/0003-0031(2003)149%5B0021:FAHHOA%5D2.0.CO%3B2},
	doi = {10.1674/0003-0031(2003)149[0021:FAHHOA]2.0.CO;2},
	abstract = {Abstract The purpose of this paper is to provide quantitative fire history information from a historically unique region, the oak barrens of the Interior Low Plateau Ecoregion. We sampled 27 post oak (Quercus stellata Wangenh.) trees from the Boone Creek watershed in southern Indiana. The period of tree-ring record ranged in calendar years from 1654 to 1999 and fire scar dates (n = 84) ranged from 1656 to 1992. The mean fire interval for the period 1656 to 1992 was 8.4 y and individual fire intervals ranged from 1 to 129 y. The average percentage of trees scarred at the site was 19\% or about 1 in 5 trees sampled. No significant relationship was identified between fire years and drought conditions however, variability in the fire record coincided with Native American migrations and Euro-American settlement periods. Temporal variability in the fire record illustrates not only the dynamic nature of anthropogenic fire regimes but also the importance of humans in culturing presettlement barrens communities.},
	number = {1},
	urldate = {2014-10-08},
	journal = {The American Midland Naturalist},
	author = {Guyette, Richard P. and Dey, Daniel C. and Stambaugh, Michael C.},
	month = jan,
