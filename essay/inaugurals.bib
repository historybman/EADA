	title = {{CERAMICS} {FROM} {THE} {KELLEY} {SITE}: {PERSPECTIVES} {ON} {THE} {ONEOTA} {TRADITION} {IN} {SOUTHEAST} {IOWA}},
	volume = {42},
	copyright = {Copyright © 1997 Plains Anthropological Society},
	issn = {0032-0447},
	shorttitle = {{CERAMICS} {FROM} {THE} {KELLEY} {SITE}},
	url = {http://www.jstor.org/stable/25669456},
	doi = {10.2307/25669456},
	abstract = {The Kelley phase is a Middle period Oneota manifestation, dating to the fifteenth century. It is one of three phases comprising a local sequence in the Kingston locality in southeast Iowa. From early to late these are the Burlington, Kelley, and Bailey Farm phases—all defined on the basis of ceramics. Similar ceramic variation occurs coevally with other southeast Iowa localities as well. The defining features of Kelley phase pottery include shoulder motifs composed of alternating or opposed trailed-line-filled triangles or vertical bands of parallel trailed lines in association with strap handles which are either attached to or below the exterior lip-rim juncture. Arguments for replacing the current Oneota horizon model with a descriptive taxonomy based on a temporal model are presented. Group continuities and issues of ethnic boundaries and ethnic continuity are examined in the contexts of the present and proposed taxonomic models with respect to the ethnographic record.},
	number = {160},
	urldate = {2013-03-20},
	journal = {Plains Anthropologist},
	author = {Tiffany, Joseph A.},
	month = may,
	year = {1997},
	note = {ArticleType: research-article / Full publication date: May 1997 / Copyright © 1997 Plains Anthropological Society},
	pages = {205--236},
	file = {JSTOR Full Text PDF:/Users/rmorriss/Library/Application Support/Firefox/Profiles/qj3yma29.default/zotero/storage/7PRARVI2/Tiffany - 1997 - CERAMICS FROM THE KELLEY SITE PERSPECTIVES ON THE.pdf:application/pdf}
}

