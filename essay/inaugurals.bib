@article{hamalainen_first_2001,
	title = {The {First} {Phase} of {Destruction}: {Killing} the {Southern} {Plains} {Buffalo}, 1790-1840},
	volume = {21},
	issn = {02757664},
	shorttitle = {The {First} {Phase} of {Destruction}},
	abstract = {Traces the gradual decline of the Comancheria bison herds on the southern Great Plains from 1790 to 1840, citing significant deterioration in bison ecology as a result of both economic and environmental pressures. Both subsistence and commercial hunting were factors in the decline of these herds. Bison numbers also diminished in response to a reduction of range by competition from exotic European species, in particular domestic horse herds. The Comanche tradition of winter hunting of pregnant cows meant that the herds could not sustain their numbers. The Great Peace of 1840 opened bison herds in the Upper Arkansas River area to new hunting groups, including "immigrant Indians" like the Cherokee, Creek, Delaware, Seminole, Kickapoo, and Shawnee. Indians managed herds through ritual with spiritual herd calls and thus, did not foresee the steady decline of their bison herds. This article challenges the long-held assertion that bison die-offs were concentrated in the mid-19th century.},
	number = {2},
	journal = {Great Plains Quarterly},
	author = {Hämäläinen, Pekka},
	month = mar,
	year = {2001},
	keywords = {AMERICAN bison, BUFFALOES, Ecology, Great Plains, HUNTING, INDIGENOUS peoples of the Americas},
