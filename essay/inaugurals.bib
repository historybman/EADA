@article{pauly_fighting_2002,
	title = {Fighting the {Hessian} {Fly}},
	volume = {7},
	issn = {10845453},
	abstract = {In the late 1770's, the gall midge began ravishing the wheat crop in New York and, subsequently, other states. To Continental army colonel, member of the American Philosophical Society, and gentleman farmer George Morgan, it was the Hessian fly, an invader as odious as the Hessians, introduced from the Old World. Entomologists at the time had trouble identifying the origins of the fly. Joseph Banks, a British scientific adviser, fearing that wheat imported from the United States could introduce the pest into Great Britain, helped shape the British ban on importing American wheat, a policy proposed by hard-liners to punish the rebellious Americans. Crop failure in Great Britain and the resulting prospect of starvation and revolution reversed the ban, as the hypothetical biological risks seemed less dangerous than the political risks of the period. The Hessian fly probably was carried in forage the British transported to the colonies during the American Revolution. The debate about the origins of the Hessian fly and questions of state policy demonstrate how nationalism and science may intertwine.},
	number = {3},
	journal = {Environmental History},
	author = {Pauly, Philip J.},
	month = jul,
