@article{tian_possible_2006,
	title = {Possible linkages of late-{Holocene} drought in the {North} {American} midcontinent to {Pacific} {Decadal} {Oscillation} and solar activity},
	volume = {33},
	issn = {1944-8007},
	url = {http://onlinelibrary.wiley.com/doi/10.1029/2006GL028169/abstract},
	doi = {10.1029/2006GL028169},
	abstract = {Paleorecords are key for evaluating the long-term patterns and controls of drought. We analyzed calcite in annually laminated sediments from a Minnesota lake for oxygen-isotopic composition (δ18O). The δ18O record of the past ∼3100 years reveals that droughts of greater severity and duration than during the 20th century occurred repeatedly, especially prior to 300 AD. Drought variability was anomalously low during the 20th century; ∼90\% of the variability values during the last 3100 years were greater than the 20th-century average. δ18O is strongly correlated with the index of the Pacific Decadal Oscillation (PDO) during the past 100 years, and periodicities of the late-Holocene δ18O record are similar to those of the PDO. Furthermore, time series of δ18O and atmospheric Δ14C are generally coherent after 700 AD. Both the Pacific climate and solar irradiance probably played a role in drought occurrence, but their effects were non-stationary through the late Holocene.},
	language = {en},
	number = {23},
	urldate = {2017-03-26},
	journal = {Geophysical Research Letters},
	author = {Tian, Jian and Nelson, David M. and Hu, Feng Sheng},
	month = dec,
	year = {2006},
	keywords = {1616 Climate variability, 3344 Paleoclimatology, 4870 Stable isotopes, 4954 Sea surface temperature, 9350 North America, deep\_history\_prairie, drought, ecology\_ecotone, Late Holocene, Pacific Decadal Oscillation},
	pages = {L23702},
	file = {Snapshot:/Users/rmorriss/Library/Application Support/Firefox/Profiles/qj3yma29.default/zotero/storage/96VEFVIM/abstract.html:text/html;Tian et al_2006_Possible linkages of late-Holocene drought in the North American midcontinent.pdf:/Users/rmorriss/Box Sync/Work/Research/Zotero/Download_articles_books/Tian et al_2006_Possible linkages of late-Holocene drought in the North American midcontinent.pdf:application/pdf}
