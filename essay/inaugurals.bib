@article{hauser_berdache_1990,
	title = {The {Berdache} and the {Illinois} {Indian} {Tribe} during the {Last} {Half} of the {Seventeenth} {Century}},
	volume = {37},
	copyright = {Copyright © 1990 The American Society for Ethnohistory},
	issn = {0014-1801},
	url = {http://www.jstor.org/stable/481936},
	doi = {10.2307/481936},
	abstract = {The Illinois berdache was a male Indian who assumed a female role in his central Algonquian society during the last half of the seventeenth century. Berdaches dressed in female clothing, assumed female occupations and restrictions, and pursued sexual relationships with both males and females. Eligible for status in a prestige system reserved for females, they then were ineligible for the advantages of status and power enjoyed by men. The Illinois neither honored nor despised berdaches. The tribespeople feared them because of the power they wielded as shamans and their acceptance as manitous. The Illinois reserved a special social position for the berdache, observed in ceremonial functions, which reflected the tribe's recognition of the berdache's special relationship to supernatural power. The institution of berdache disappeared after 1698.},
	number = {1},
	urldate = {2013-11-04},
