@phdthesis{shackelford_crossroads:_2004,
	address = {United States -- Indiana},
	type = {Ph.{D}.},
	title = {On a crossroads: {American} {Indian} prehistory and history in the {Confluence} {Region}},
	copyright = {Copyright UMI - Dissertations Publishing 2004},
	shorttitle = {On a crossroads},
	url = {http://search.proquest.com.proxy2.library.illinois.edu/docview/305201488/abstract/13CF85F436B1D576305/1?accountid=14553},
	abstract = {When Lewis and Clark and their command journeyed westward from St. Louis in 1804, they cemented that city's reputation as the "Gateway to the West" in the American imagination. But before this portion of the Mississippi drainage became an American gateway, it was a Native American crossroads. The confluence of the Illinois, Missouri, and Ohio rivers with the Mississippi River repeatedly made the region a frontier connecting Native American communities located throughout the interior of the continent. Since at least 9,000 B.C. Native Americans migrated, traded, and exchanged ideas, practices, and beliefs across this region. Native Americans also repeatedly settled in the region due to the variety of environmental boons provided by the rivers and their surrounding landscape. Thus a succession of Native American communities learned to navigate the possibilities and perils of living on a cross-cultural, multi-ethnic borderland prior to the arrival of Europeans. When Europeans first arrived in the Confluence Region they encountered Illinois peoples. The Illinois were recent newcomers drawn to this land by buffalo herds and interethnic relationships. Initially the Illinois sought to integrate the French into their set of frontier relationships, but they soon found that life on multiethnic frontier could be dynamically unstable. The same forces of geography and environment that had frequently drawn Native Americans to the Confluence Region drew large numbers of French settlers during the eighteenth century. Amid a growing colonial population, the Illinois found their value as both diplomatic and economic partners to the French eroding. Some Illinois incorporated themselves into the new colonial communities of the Confluence Region. Others abandoned the region to join other Indian communities less vulnerable to the colonial dominance. A few Illinois succeeded in maintaining an autonomous ethnic identity, but found to do so they had to remove themselves from the Confluence Region. By studying a succession of prehistoric and historic Native American communities in the Confluence Region, this dissertation demonstrates that prehistoric Native America was not a land and time without history. Native Americans had a meaningful past to draw upon as they faced a world changed by the arrival of Europeans and Africans.},
	language = {English},
	urldate = {2013-03-22},
	school = {Indiana University},
	author = {Shackelford, Alan G.},
	year = {2004},
	file = {Snapshot:/Users/rmorriss/Library/Application Support/Firefox/Profiles/qj3yma29.default/zotero/storage/9XBAREDC/1.html:text/html}
}

@incollection{edmunds_illinois_2008,
